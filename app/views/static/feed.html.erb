	<script>
		/* list container node */
		const resultsList = document.getElementById('resultsList')

		const getInitialData = async () => {
			/* fetch data */
			const zillowURI =
				'https://api.apify.com/v2/datasets/cdcjgxj4POsh9Faz1/items?token=apify_api_vx9jttk1Q2m6Kg7fXLKbuHFTh84E7f3SMYWE'

			const res = await fetch(zillowURI)

			return await res.json()
		}

		getInitialData().then(async (data) => {
			const options = {
				method: 'GET',
				headers: {
					'X-RapidAPI-Key':
						'3fddde8351msh7a6c4aa7290f1cbp17ac8ejsn3b5743625c0c',
					'X-RapidAPI-Host': 'walk-score.p.rapidapi.com',
				},
			}

			data.forEach(async (result) => {
				const walkScoreURI = `https://walk-score.p.rapidapi.com/score?lat=${result.latitude}&address=${result.address.streetAddress}&wsapikey=ffd1c56f9abcf84872116b4cc2dfcf31&lon=${result.longitude}&format=json`

				const res = await fetch(walkScoreURI, options)

				res
					.json()
					.then((walkData) => {
						const walkScore = walkData.walkscore

						/* creating DOM elements for card */
						const outerParentDiv = document.createElement('div')
						outerParentDiv.className = 'col-xl-4 col-sm-6 col-12'

						const innerParentDiv = document.createElement('div')
						innerParentDiv.className =
							'ltn__product-item ltn__product-item-4 ltn__product-item-5 text-center---'

						const productImageDiv = document.createElement('div')
						productImageDiv.className = 'product-img'

						const productDetails = document.createElement('a')
						productDetails.href = '#'

						const productImage = document.createElement('img')
						productImage.src = result.photos[0]
						productImage.referrerPolicy = 'no-referrer'
						productImage.style.width = '100%'
						productImage.style.aspectRatio = '328 / 251'

						const productInfo = document.createElement('div')
						productInfo.className = 'product-info'

						const productBadge = document.createElement('div')
						productBadge.className = 'product-badge'

						const badgeUl = document.createElement('ul')

						const badgeLi = document.createElement('li')
						/* this might be a typo check back later */
						badgeLi.className = 'sale-badg'
						badgeLi.textContent = result.homeStatus.replaceAll('_', ' ')

						const productTitle = document.createElement('h2')
						productTitle.className = 'product-title'

						const productTitleLink = document.createElement('a')
						productTitleLink.href = '#'
						productTitleLink.textContent = result.homeType.replaceAll('_', ' ')

						const productImageLocation = document.createElement('div')
						productImageLocation.className = 'product-img-location'

						const productImageLocationUl = document.createElement('ul')

						const productImageLocationLi = document.createElement('li')

						const productImageLocationA = document.createElement('a')
						productImageLocationA.href = '#'

						const productImageLocationI = document.createElement('I')
						productImageLocationI.className = 'flaticon-pin'

						const streetAddress = document.createTextNode(
							` ${result.address.streetAddress}`
						)

						const listItemUl = document.createElement('ul')
						listItemUl.className =
							'ltn__list-item-2--- ltn__list-item-2-before--- ltn__plot-brief'

						const listItemLi1 = document.createElement('li')

						const listItemSpan1 = document.createElement('span')
						listItemSpan1.textContent = `${result.bedrooms} `

						const listItemText1 = document.createTextNode('Bed')

						const listItemLi2 = document.createElement('li')

						const listItemSpan2 = document.createElement('span')
						listItemSpan2.textContent = `${result.bathrooms} `

						const listItemText2 = document.createTextNode('Bath')

						const listItemLi3 = document.createElement('li')

						const listItemSpan3 = document.createElement('span')
						listItemSpan3.textContent = `${result.livingArea} `

						const listItemText3 = document.createTextNode('Square Ft')

						const listItemLi4 = document.createElement('li')

						const listItemSpan4 = document.createElement('span')
						listItemSpan4.textContent = `${walkScore} `

						const listItemText4 = document.createTextNode('Walk Score')

						const productInfoBottom = document.createElement('div')
						productInfoBottom.className = 'product-info-bottom'

						const productPrice = document.createElement('div')
						productPrice.className = 'product-price'

						const productPriceSpan = document.createElement('span')
						productPriceSpan.textContent = `$${result.price.toLocaleString()}`

						/* appending created elements to the DOM */
						resultsList.appendChild(outerParentDiv).appendChild(innerParentDiv)

						innerParentDiv
							.appendChild(productImageDiv)
							.appendChild(productDetails)
							.appendChild(productImage)

						innerParentDiv.appendChild(productInfo)

						productInfo
							.appendChild(productBadge)
							.appendChild(badgeUl)
							.appendChild(badgeLi)

						productInfo.appendChild(productTitle).appendChild(productTitleLink)

						productInfo
							.appendChild(productImageLocation)
							.appendChild(productImageLocationUl)
							.appendChild(productImageLocationLi)
							.appendChild(productImageLocationA)
							.append(productImageLocationI, streetAddress)

						productInfo.appendChild(listItemUl)

						listItemUl
							.appendChild(listItemLi1)
							.append(listItemSpan1, listItemText1)
						listItemUl
							.appendChild(listItemLi2)
							.append(listItemSpan2, listItemText2)
						listItemUl
							.appendChild(listItemLi3)
							.append(listItemSpan3, listItemText3)
						listItemUl
							.appendChild(listItemLi4)
							.append(listItemSpan4, listItemText4)

						innerParentDiv
							.appendChild(productInfoBottom)
							.appendChild(productPrice)
							.appendChild(productPriceSpan)
					})
					.catch(console.log)
			})
		})

		document.getElementById('filterButton').addEventListener('click', () => {
